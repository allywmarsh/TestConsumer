{
  "routes": [
    {
        "route": "/admin/*",
        "allowedRoles": [
            "authenticated"
        ]
    },
    {
        "route": "/login",
        "rewrite": "/.auth/login/aadb2c"
    },
    {
        "route": "/logout",
        "redirect": "/.auth/logout"
    }
],
    "auth": {
      "identityProviders": {
        "customOpenIdConnectProviders": { 
          "aadb2c": {
            "registration": {
              "clientIdSettingName": "AADB2C_PROVIDER_CLIENT_ID",
              "clientCredential": {
                "clientSecretSettingName": "AADB2C_PROVIDER_CLIENT_SECRET"
              },
              "openIdConnectConfiguration": {
                "wellKnownOpenIdConfiguration": "https://B00645579CW2.b2clogin.com/B00645579CW2/B2C_1_CW2ConsumerUserFlow/v2.0/.well-known/openid-configuration"
              }
            },
            "login": {
              "nameClaimType": "emails",
              "scopes": ["openid"]
            }
          }
        }
      }
    },
    "platformErrorOverrides": [
      {
        "errorType": "NotFound",
        "serve": "/custom-404.html"
      },
      {
        "errorType": "Unauthenticated",
        "serve": "/login"
      }
    ]
  }
